<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.proiect_colectiv.model.User" table="Users">
        <id name="ID"  column="id">
            <generator class="increment"/>
        </id>
        <property
                name="username"
                column="username"/>
        <property
                name="password"
                column="password"/>
    </class>
</hibernate-mapping>